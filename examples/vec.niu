struct Vec<T> $${std::vector<$ty(T)>}$$ {
  fn new() -> Self $${std::vector<$ty(T)>()}$$
  fn push(self: &Self, t: T) -> bool $${$arg(self)->push_back($arg(t))}$$
  fn get(self: &Self, i: u64) -> T $${$arg(self)->at($arg(i))}$$
}

fn main() -> Vec<i64> {
  let vec = Vec::new();
  (&vec).push(1i64);
  vec
}
